generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Quote {
  id        Int      @id @default(autoincrement())
  content   String   @db.Text
  author    String?
  category  String?
  isFavorite Boolean @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
